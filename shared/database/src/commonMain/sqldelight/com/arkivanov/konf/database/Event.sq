
CREATE TABLE EventEntity (
    title TEXT,
    description TEXT,
    imageUrl TEXT,
    startDate INTEGER,
    endDate INTEGER,
    timeZone TEXT,
    locationLatitude REAL,
    locationLongitude REAL,
    locationDescription TEXT,
    websiteUrl TEXT
);

get:
SELECT * FROM EventEntity;

clear:
DELETE FROM EventEntity;

put:
INSERT INTO EventEntity VALUES ?;
